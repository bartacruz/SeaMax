           <value>-0.7</value>
           <!--from c172p-->
           <table>
             <independentVar lookup="row">aero/alpha-deg</independentVar>
             <tableData>
               20    1
               25    0.6
               35    0.4
               45    0.5
               55    0.4
               65    0.2
               90    0.1
             </tableData>
           </table>
       </product>
    </function>

    <function name="aero/moment/Pitch_elevator">
