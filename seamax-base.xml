<?xml version="1.0" encoding="ISO-8859-1"?>

<PropertyList>
    <sim>
        <model>
            <path archive="y">Aircraft/SeaMax/Models/seamax.xml</path>
            <livery>
                <file type="string"></file>
            </livery>
            <door-positions>
                <canopy>
                    <position-norm type="float">0</position-norm>
                </canopy>
                <crew>
                    <position-norm type="float">0</position-norm>
                </crew>
            </door-positions>
        </model>
        
        <view n="0">
            <name type="string">Cockpit View</name>
            <internal archive="y">true</internal>
            <config>
                <x-offset-m archive="y"> -0.27 </x-offset-m>                <!-- Right -->
                <y-offset-m archive="y"> 1.18 </y-offset-m>                <!-- Up    -->
                <z-offset-m archive="y"> 1.8 </z-offset-m>                <!-- Back  -->
                <pitch-offset-deg type="double">-10</pitch-offset-deg>
                <default-field-of-view-deg type="double">85</default-field-of-view-deg>
            </config>
        </view>
        
        <view n="100">
            <name>Inspection</name>
            <enabled type="bool">true</enabled>
            <type>lookat</type>
            <config>
                <from-model type="bool">false</from-model>
                <from-model-idx type="int">0</from-model-idx>
                <heading-offset-deg>248</heading-offset-deg>
                <pitch-offset-deg type="double">-12</pitch-offset-deg>
                <eye-lat-deg-path>/position/latitude-deg</eye-lat-deg-path>
                <eye-lon-deg-path>/position/longitude-deg</eye-lon-deg-path>
                <eye-alt-ft-path>/position/altitude-ft</eye-alt-ft-path>
                <eye-heading-deg-path>/orientation/heading-deg</eye-heading-deg-path>
                <!---->
                <at-model type="bool">true</at-model>
                <at-model-idx type="int">0</at-model-idx>
                <at-model-heading-damping type="double">0.0</at-model-heading-damping>
    
                <target-x-offset-m type="double"> 0</target-x-offset-m>
                <target-y-offset-m type="double">0.0</target-y-offset-m>
                <target-z-offset-m type="double">3.0</target-z-offset-m>
    
                <ground-level-nearplane-m type="double">1</ground-level-nearplane-m>
                <default-field-of-view-deg type="double">75</default-field-of-view-deg>
    
                <x-offset-m type="double">0.0</x-offset-m>
                <y-offset-m type="double">2.0</y-offset-m>
                <z-offset-m type="double">-7.0</z-offset-m>
    
                <!-- <x-offset-m type="double">-1</x-offset-m>
                <y-offset-m type=double">1.7</y-offset-m>
                <z-offset-m type="double">2</z-offset-m> -->
            </config>
        </view>
        <view n="101">
            <name type="string">Copilot View</name>
            <internal archive="y">true</internal>
            <config>
                <from-model type="bool">true</from-model>
                <from-model-idx type="int">1</from-model-idx>
                <dynamic-view type="bool">true</dynamic-view>
                <x-offset-m archive="y"> 0.27 </x-offset-m>                <!-- Right -->
                <y-offset-m archive="y"> 1.13 </y-offset-m>                <!-- Up    -->
                <z-offset-m archive="y"> 1.54 </z-offset-m>                <!-- Back  -->
                <pitch-offset-deg type="double">-10</pitch-offset-deg>
                <default-field-of-view-deg type="double">100</default-field-of-view-deg>
            </config>
        </view>
        <view n="102">
            <name type="string">Overhead Panel</name>
            <internal archive="y">true</internal>
            <config>
                <from-model type="bool">true</from-model>
                <from-model-idx type="int">0</from-model-idx>
                <dynamic-view type="bool">false</dynamic-view>
                <x-offset-m > 0 </x-offset-m> <!-- Right -->
                <y-offset-m > 0.71 </y-offset-m> <!-- Up    -->
                <z-offset-m > 1.61 </z-offset-m> <!-- Back  -->
                <pitch-offset-deg type="double">90</pitch-offset-deg>    
                <field-of-view type="double">67</field-of-view>
                <default-field-of-view-deg type="double"></default-field-of-view-deg>
                <default-pitch-deg type="double">85</default-pitch-deg>
                <default-heading-deg type="double">0</default-heading-deg>
            </config>
        </view>
        <view n="103">
            <name type="string">IPad View</name>
            <internal archive="y">true</internal>
            <config>
                <from-model type="bool">true</from-model>
                <from-model-idx type="int">0</from-model-idx>
                <dynamic-view type="bool">true</dynamic-view>
                <x-offset-m archive="y"> 0 </x-offset-m>                <!-- Right -->
                <y-offset-m archive="y"> 0.888 </y-offset-m>                <!-- Up    -->
                <z-offset-m archive="y"> 1.5 </z-offset-m>                <!-- Back  -->
                <pitch-offset-deg type="double">0</pitch-offset-deg>
                <default-field-of-view-deg type="double">64.6</default-field-of-view-deg>
            </config>
        </view>
        
        <previews>
            <preview>
                <type>cockpit</type>
                <path>Previews/seamax1.jpg</path>
                <splash type="bool">true</splash>
            </preview>
        </previews>
        <flaps include="Systems/flaps.xml" />
        <sound>
            <path>Sounds/seamax-sound.xml</path>
        </sound>
        <weight n="0">
            <name type="string">Pilot</name>
            <weight-lb type="double">160</weight-lb>
            <max-lb type="int">240</max-lb>
            <min-lb type="int">0</min-lb>
        </weight>
        <weight n="1">
            <name type="string">Co-Pilot</name>
            <weight-lb type="double">0</weight-lb>
            <max-lb type="int">240</max-lb>
            <min-lb type="int">0</min-lb>
        </weight>
        <weight n="2">
            <name type="string">Storage left</name>
            <weight-lb type="double">10</weight-lb>
            <max-lb type="int">45</max-lb>
            <min-lb type="int">0</min-lb>
        </weight>
        <weight n="3">
            <name type="string">Storage right</name>
            <weight-lb type="double">10</weight-lb>
            <max-lb type="int">45</max-lb>
            <min-lb type="int">0</min-lb>
        </weight>
        <weight n="4">
            <name type="string">Ballast</name>
            <weight-lb type="double">0</weight-lb>
            <max-lb type="int">15</max-lb>
            <min-lb type="int">0</min-lb>
        </weight>
        <systems include="Systems/rules.xml">
            <electrical>
                <path></path>
            </electrical>
        </systems>
    </sim>
    <input include="Systems/seamax-keyboard.xml" />
    <payload>
        <weight>
            <name type="string">Pilot</name>
            <weight-lb alias="/fdm/jsbsim/inertia/pointmass-weight-lbs[0]" />
            <arm-in alias="/fdm/jsbsim/inertia/pointmass-location-X-inches[0]" />
            <min-lb type="double">0.0</min-lb>
            <max-lb type="double">300.0</max-lb>
        </weight>
        <weight>
            <name type="string">Co-Pilot</name>
            <weight-lb alias="/fdm/jsbsim/inertia/pointmass-weight-lbs[1]" />
            <arm-in alias="/fdm/jsbsim/inertia/pointmass-location-X-inches[1]" />
            <min-lb type="double">0.0</min-lb>
            <max-lb type="double">300.0</max-lb>
        </weight>
        <weight>
            <name type="string">Storage pilot</name>
            <weight-lb alias="/fdm/jsbsim/inertia/pointmass-weight-lbs[2]" />
            <arm-in alias="/fdm/jsbsim/inertia/pointmass-location-X-inches[2]" />
            <min-lb type="double">0.0</min-lb>
            <max-lb type="double">45</max-lb>
        </weight>
        <weight>
            <name type="string">Storage co-pilot</name>
            <weight-lb alias="/fdm/jsbsim/inertia/pointmass-weight-lbs[3]" />
            <arm-in alias="/fdm/jsbsim/inertia/pointmass-location-X-inches[3]" />
            <min-lb type="double">0.0</min-lb>
            <max-lb type="double">45</max-lb>
        </weight>
    </payload>
    <engines>
        <engine>
            <t-pressure-diff type="float">0</t-pressure-diff>
            <t-pressure-norm type="float">0</t-pressure-norm>
            <fuel-pressure-psi-lag type="float">0</fuel-pressure-psi-lag>
        </engine>
        <active-engine>
            <rpm alias="/engines/engine/rpm" />
        </active-engine>
    </engines>
    <controls>
        <electric>
            <battery-switch type="bool">1</battery-switch>
            <master-switch type="bool">0</master-switch>
        </electric>
        <fuel>
            <tank n="0">
                <fuel_selector type="bool">0</fuel_selector>
            </tank>
            <tank n="1">
                <fuel_selector type="bool">0</fuel_selector>
            </tank>
            <tank n="2">
                <boost-pump type="bool">false</boost-pump>
                <fuel_selector type="bool">1</fuel_selector>
            </tank>
        </fuel>
        <engines>
            <engine>
                <mixture type="float">1.0</mixture>
                <hackman type="bool">false</hackman>
            </engine>
        </engines>
        <gear>
            <gear-lever-norm type="float">1.0</gear-lever-norm>
            <water-rudder type="bool">0</water-rudder>
            <water-rudder-down type="double">0</water-rudder-down>
            <jacks-operable type="bool">1</jacks-operable>
            <jacks-on type="bool">0</jacks-on>
        </gear>
        <switches>
            <bilge-pump type="bool">false</bilge-pump>
            <master-cover type="bool">false</master-cover>
            <starter type="bool">false</starter>
        </switches>
    </controls>
    <seamax>
        <debug type="bool">0</debug>
    </seamax>

    <nasal>
        <updateloop>
            <file>Aircraft/Generic/updateloop.nas</file>
        </updateloop>
        <electric>
            <file>Aircraft/SeaMax/Nasal/electric.nas</file>
        </electric>
        <atr833_ii>
			<file>Models/Interior/Instruments/ATR833-II/atr833-ii.nas</file>
		</atr833_ii>
        <seamax>
            <file>Aircraft/SeaMax/Nasal/init.nas</file>
            <file>Aircraft/SeaMax/Nasal/seamax-electric.nas</file>
        </seamax>
        <hackman>
            <file>Aircraft/SeaMax/Nasal/hackman.nas</file>
        </hackman>
        <canvas_map>
            <file>Aircraft/SeaMax/Models/Interior/Instruments/ipad/canvas_map.nas</file>
        </canvas_map>
    </nasal>
</PropertyList>