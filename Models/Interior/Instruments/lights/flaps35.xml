<?xml version="1.0" encoding="ISO-8859-1"?>

<PropertyList>

  <path>led.ac</path>

  <animation>
    <object-name>base</object-name>
    <object-name>led</object-name>
    <object-name>cover</object-name>
  </animation>

  <animation>
    <type>noshadow</type>
    <object-name>base</object-name>
  </animation>

  <animation>
    <type>select</type>
    <object-name>led</object-name>
    <condition>
      <property>/systems/electrical/outputs/main-bus</property>
      <equals>
        <property>surface-positions/flap-pos-norm</property>
        <value>1</value>
      </equals>
    </condition>
  </animation>

  <animation>
    <type>material</type>
    <object-name>led</object-name>
    <emission>
      <red>  1 </red>
      <green> 1 </green>
      <blue> 0.2 </blue>
    </emission>
  </animation>

</PropertyList>