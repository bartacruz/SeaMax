<?xml version="1.0" encoding="ISO-8859-1"?>

<PropertyList>
  
  <path>switch.ac</path>  
  <animation>
    <!-- Objets opaques -->
    <object-name>base</object-name>
    <object-name>on-off</object-name>
    
    <!-- Objets transparents -->
  </animation>
  
  <animation>
    <object-name>on-off</object-name>
    <type>rotate</type>
    <property>/controls/switches/noop</property>
    <factor> 35 </factor>
    <min-deg>  0 </min-deg>
    <max-deg> 35 </max-deg>
    <axis>
      <x>  0 </x>
      <y> -1 </y>
      <z>  0 </z>
    </axis>
  </animation>

</PropertyList>
