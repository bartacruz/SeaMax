<?xml version="1.0" encoding="UTF-8"?>
<PropertyList>
    <path>rotax912-oil-press.ac</path>
    <effect>
        <inherits-from>../../../Effects/Glass/glass</inherits-from>
        <object-name>oil press glass</object-name>
    </effect>
    <animation>
        <type>material</type>
        <object-name>oil press</object-name>
        <object-name>oil press needle</object-name>
        <condition>
            <not>
                <property>sim/rendering/rembrandt/enabled</property>
            </not>
        </condition>
        <emission>
            <red> 0.07 </red>
            <green> 0.04 </green>
            <blue> 0.04 </blue>
            <factor-prop>systems/electrical/outputs/instrument-lights</factor-prop>
        </emission>
    </animation>

    <animation>
		<type>rotate</type>
		<object-name>oil press needle</object-name>
		<property>/engines/engine/oil-pressure-psi</property>
		<interpolation>
			<entry>
				<ind>   0 </ind>
				<dep>   0 </dep>
			</entry>
			<entry>
				<ind>  30</ind>
				<dep>  21.4 </dep>
			</entry>
			<entry>
				<ind>  70 </ind>
				<dep>  50.8 </dep>
			</entry>
			<entry>
				<ind>  100 </ind>
				<dep>  68.8 </dep>
			</entry>
			<entry>
				<ind>  145 </ind>
				<dep> 88.4 </dep>
			</entry>
		</interpolation>
		<center>
			<x-m>0.0032</x-m>
			<y-m>0.0137</y-m>
			<z-m>0</z-m>
				
		</center>
		<axis>
			<x> -1 </x>
			<y>  0 </y>
			<z>  0 </z>
		</axis>
	</animation>
</PropertyList>