<?xml version="1.0" encoding="UTF-8"?>
<PropertyList>
    <path>rotax912-fuel-press.ac</path>
    <effect>
        <inherits-from>../../../Effects/Glass/glass</inherits-from>
        <object-name>fuel press glass</object-name>
    </effect>
    <animation>
        <type>material</type>
        <object-name>fuel press</object-name>
        <object-name>fuel press needle</object-name>
        <condition>
            <not>
                <property>sim/rendering/rembrandt/enabled</property>
            </not>
        </condition>
        <emission>
            <red> 0.07 </red>
            <green> 0.04 </green>
            <blue> 0.04 </blue>
            <factor-prop>systems/electrical/outputs/instrument-lights</factor-prop>
        </emission>
    </animation>

    <animation>
		<type>rotate</type>
		<object-name>fuel press needle</object-name>
		<property>/engines/engine/fuel-pressure-psi</property>
		<interpolation>
			<entry>
				<ind>   0 </ind>
				<dep>   0 </dep>
			</entry>
			<entry>
				<ind>  1.5 </ind>
				<dep>  53.86 </dep>
			</entry>
			<entry>
				<ind>  3 </ind>
				<dep>  108 </dep>
			</entry>
			<entry>
				<ind>  4.5 </ind>
				<dep>  161.5 </dep>
			</entry>
			<entry>
				<ind>  6.0 </ind>
				<dep> 216 </dep>
			</entry>
			<entry>
				<ind>  14.5 </ind>
				<dep> 270 </dep>
			</entry> <!-- mechanical stop -->
		</interpolation>
		<axis>
			<x> -1 </x>
			<y>  0 </y>
			<z>  0 </z>
		</axis>
	</animation>
</PropertyList>