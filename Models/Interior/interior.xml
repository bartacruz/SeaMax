    <property>/controls/lighting/instrument-lights</property>
    <factor> 35 </factor>
    <center>
      <x-m>1.53</x-m>
      <y-m>0.005</y-m>
      <z-m>1.3</z-m>
    </center>
    <axis>
      <x> -1 </x>
      <y> 0 </y>
      <z> 0 </z>
    </axis>
  </animation>

  <!-- Fuel switch left -->
  <animation>
    <type>pick</type>
    <object-name>fuel switch left</object-name>
    <action>
      <button>0</button>
      <repeatable>false</repeatable>
      <binding>
        <command>property-toggle</command>
        <property>/controls/fuel/tank[0]/fuel_selector</property>
      </binding>
    </action>
    <hovered>
      <binding>
        <command>set-tooltip</command>
        <tooltip-id>left-fuel-switch</tooltip-id>
        <label>Fuel left: %s</label>
        <property>/controls/fuel/tank[0]/fuel_selector</property>
        <mapping>nasal</mapping>
        <script>
          var m = arg[0];
          if (m == 1) return 'ON';
          return 'OFF';
        </script>
      </binding>
    </hovered>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>fuel switch left</object-name>
    <property>/controls/fuel/tank[0]/fuel_selector</property>
    <factor>90</factor>
    <axis>
      <x1-m> 2.0368</x1-m>
      <y1-m>-0.40216</y1-m>
      <z1-m> 1.2298</z1-m>
      <x2-m> 2.0347</x2-m>
      <y2-m>-0.39236</y2-m>
      <z2-m> 1.2217</z2-m>


    </axis>
  </animation>

