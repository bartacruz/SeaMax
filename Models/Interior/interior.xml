  <animation>
    <type>rotate</type>
    <object-name>fuel switch right</object-name>
    <property>/controls/fuel/tank[1]/fuel_selector</property>
    <factor>-90</factor>
    <axis>
      <x1-m> 2.0359</x1-m>
      <y1-m> 0.40093</y1-m>
      <z1-m> 1.2312</z1-m>
      <x2-m> 2.0347</x2-m>
      <y2-m> 0.39236</y2-m>
      <z2-m> 1.2217</z2-m>

    </axis>
  </animation>

  <model>
    <path>Instruments/magneto-switch/magneto-switch.xml</path>
    <offsets>
      <x-m>1.77</x-m>
      <y-m> 0.032</y-m>
      <z-m> 1.302 </z-m>
      <pitch-deg>90</pitch-deg>
      <heading-deg>180</heading-deg>
    </offsets>
  </model>
  <model>
    <path>Instruments/switch/master.xml</path>
    <offsets>
      <x-m>1.77</x-m>
      <y-m>-0.032</y-m>
      <z-m> 1.285 </z-m>
      <pitch-deg>90</pitch-deg>
    </offsets>
