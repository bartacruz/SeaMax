  <!-- Yoke-->
  <animation>
    <type>rotate</type>
    <object-name>yoke</object-name>
    <object-name>yoke cover</object-name>
    <property>controls/flight/elevator</property>
    <interpolation>
      <entry>
        <ind>-1</ind>
        <dep>-33</dep>
      </entry>
      <entry>
        <ind> 0</ind>
        <dep> 0</dep>
      </entry>
      <entry>
        <ind> 1</ind>
        <dep> 33</dep>
      </entry>
    </interpolation>
    <center>
      <x-m> 1.27</x-m>
      <y-m> 0</y-m>
      <z-m> 0.5</z-m>
    </center>
    <axis>
      <x> 0</x>
      <y>-1</y>
      <z> 0</z>
    </axis>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>yoke</object-name>
    <object-name>yoke cover</object-name>
    <property>controls/flight/aileron</property>
    <interpolation>
      <entry>
        <ind>-1</ind>
        <dep>-33</dep>
      </entry>
      <entry>
        <ind> 0</ind>
        <dep> 0</dep>
      </entry>
      <entry>
        <ind> 1</ind>
        <dep> 33</dep>
      </entry>
    </interpolation>
    <center>
      <x-m> 1.27</x-m>
      <y-m> 0</y-m>
      <z-m> 0.5</z-m>
    </center>
    <axis>
      <x>-1</x>
      <y> 0</y>
      <z> 0</z>
    </axis>
  </animation>

  <!-- Overhead panel -->

  <!-- Gear lever -->
  <animation>
    <type>rotate</type>
    <object-name>gear lever</object-name>
    <object-name>gear lever handle</object-name>
    <property>/controls/gear/gear-lever-norm</property>
    <factor>50</factor>
    <axis>
      <x>0</x>
      <y>-1</y>
      <z>0</z>
    </axis>
    <center>
      <x-m>1.46</x-m>
      <y-m>0.0</y-m>
      <z-m>1.33</z-m>
    </center>
  </animation>

  <animation>
    <type>pick</type>
    <object-name>gear lever</object-name>
    <object-name>gear lever handle</object-name>
    <action>
      <button>0</button>
      <repeatable>false</repeatable>
      <binding>
        <command>property-toggle</command>
        <property>controls/gear/gear-down</property>
      </binding>
      <!-- <binding>
        <command>property-interpolate</command>
        <property>/controls/gear/gear-lever-norm</property>
        <property>controls/gear/gear-down</property>
        <time>0.5</time>
      </binding> -->
    </action>
  </animation>

  <!-- Landing light-->
  <animation>
    <type>material</type>
    <object-name>button landing</object-name>
    <emission>
      <factor-prop>systems/electrical/outputs/landing-light</factor-prop>
      <red> 0.002 </red>
      <green> 0.036 </green>
      <blue> 0.002 </blue>
    </emission>
  </animation>

  <animation>
    <type>pick</type>
    <visible>true</visible>
    <object-name>button landing</object-name>
    <action>
      <button>0</button>
      <repeatable>false</repeatable>
      <binding>
        <command>property-toggle</command>
        <property>/controls/lighting/landing-light</property>
      </binding>
      <binding>
        <command>property-toggle</command>
        <property>sim/model/click-overhead</property>
      </binding>
    </action>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>button landing</object-name>
    <property>/controls/lighting/landing-light</property>
    <factor> 35 </factor>
    <center>
      <x-m>1.53</x-m>
      <y-m>0.005</y-m>
      <z-m>1.3</z-m>
    </center>
    <axis>
      <x> -1 </x>
      <y> 0 </y>
      <z> 0 </z>
    </axis>
  </animation>

  <!-- Strobe -->
  <animation>
    <type>material</type>
    <object-name>button strobe</object-name>
    <emission>
      <factor-prop>systems/electrical/outputs/strobe-lights</factor-prop>
      <red> 0.002 </red>
      <green> 0.036 </green>
      <blue> 0.002 </blue>
    </emission>
  </animation>

  <animation>
    <object-name>button strobe</object-name>
    <type>rotate</type>
    <property>controls/lighting/strobe-lights</property>
    <factor> 35 </factor>
    <min-deg> 0 </min-deg>
    <max-deg> 35 </max-deg>
    <center>
      <x-m>1.53</x-m>
      <y-m>0.005</y-m>
      <z-m>1.3</z-m>
    </center>
    <axis>
      <x> -1 </x>
      <y> 0 </y>
      <z> 0 </z>
    </axis>
  </animation>

  <animation>
    <type>pick</type>
    <object-name>button strobe</object-name>
    <visible>true</visible>
    <action>
      <button>0</button>
      <repeatable>false</repeatable>
      <binding>
        <command>property-toggle</command>
        <property>controls/lighting/strobe-lights</property>
      </binding>
      <binding>
        <command>property-toggle</command>
        <property>sim/model/click-overhead</property>
      </binding>
    </action>
  </animation>

