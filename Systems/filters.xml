    <filter>
        <type>gain</type>
        <gain>1.0</gain>
        <input>
            <expression>
                <difference>
                    <property>engines/engine/egt-degf</property>
                    <property>environment/temperature-degf</property>
                </difference>
            </expression>
        </input>
        <output>engines/engine/t-egt-abs</output>

    </filter>

    <filter>
        <type>gain</type>
        <gain>1.0</gain>
        <input>
            <expression>
                <div>
                    <property>engines/engine/mp-inhg</property>
                    <property>environment/pressure-inhg</property>
                </div>
            </expression>
        </input>
        <output>engines/engine/t-pressure-diff</output>
    </filter>
    <filter>
        <type>gain</type>
        <gain>1.0</gain>
        <input>
            <expression>
                <product>
                    <property>controls/engines/engine/throttle</property>
                    <property>engines/engine/mp-inhg</property>
                </product>
            </expression>
        </input>
        <output>engines/engine/t-pressure-norm</output>
    </filter>

