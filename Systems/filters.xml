<?xml version="1.0" encoding="ISO-8859-1"?>
<PropertyList>
    <filter>
        <type>gain</type>
        <gain>1.0</gain>
        <input>
            <expression>
                <product>
                    <property>engines/engine/rpm</property>
                    <value>0.411</value>
                </product>
            </expression>
        </input>
        <output>engines/engine/prop-rpm</output>
    </filter>
