        <stall aoa="16" width="8" peak="1.5"/>
        <flap0 start="0.14" end="0.56" lift="1.2" drag="1.25"/>
        <flap1 start="0.561" end="0.960" lift="1.2" drag="1.2"/>

        <control-input control="FLAP0" axis="/controls/flight/flaps"/>
